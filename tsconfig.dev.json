{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["DOM", "ES2019"],
    "jsx": "react",
    "moduleResolution": "node",
    "module": "commonjs",
    "noUnusedLocals": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "ES2015"
  }
}
