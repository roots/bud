{
  "name": "bud",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "description": "⚡️ Lightning fast frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git"
  },
  "license": "MIT",
  "engines": {
    "node": "16.18.0"
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.22.19",
    "npm": "8.19.2"
  },
  "bud": {
    "extensions": {
      "discovery": false
    }
  },
  "workspaces": {
    "packages": [
      "sources/@roots/*",
      "sources/@repo/*",
      "sources/@roots/*/test/**/*",
      "sources/@roots/*/src/__fixtures__/*",
      "examples/node-api",
      "tests/util/project",
      "tests/reproductions/*"
    ]
  },
  "type": "module",
  "dependencies": {
    "@repo/constants": "workspace:sources/@repo/constants",
    "@repo/logger": "workspace:sources/@repo/logger",
    "@repo/test-kit": "workspace:sources/@repo/test-kit",
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@skypack/package-check": "0.2.2",
    "@types/eslint": "8.4.10",
    "@types/express": "4.17.15",
    "@types/fs-extra": "11.0.1",
    "@types/lodash": "4.14.191",
    "@types/node": "16.18.11",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.1",
    "@vitest/coverage-istanbul": "0.26.3",
    "@vitest/ui": "0.26.3",
    "eslint": "8.31.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "execa": "6.1.0",
    "fs-extra": "11.1.0",
    "import-meta-resolve": "2.2.0",
    "lodash": "4.17.21",
    "playwright": "1.29.2",
    "prettier": "2.8.1",
    "syncpack": "8.4.11",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.9.4",
    "vite": "4.0.3",
    "vitest": "0.26.3",
    "webpack": "5.75.0"
  }
}
