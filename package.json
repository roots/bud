{
  "name": "bud",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "description": "⚡️ Lightning fast frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git"
  },
  "license": "MIT",
  "engines": {
    "node": "^16.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.18"
  },
  "workspaces": {
    "packages": [
      "sources/@roots/*",
      "tests/util/project",
      "sources/@repo/*"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@repo/constants": "workspace:*",
    "@repo/logger": "workspace:*",
    "@repo/test-kit": "workspace:sources/@repo/test-kit",
    "@roots/bud-build": "workspace:sources/@roots/bud-build",
    "@types/express": "^4",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.34",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "commitlint-config-gitmoji": "2.2.5",
    "cz-customizable": "6.3.0",
    "eslint": "8.15.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "jest": "28.1.0",
    "prettier": "2.6.0",
    "ts-jest": "28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "tslib": "2.4.0"
  }
}
