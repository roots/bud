{
  "name": "@roots/bud-compress",
  "version": "0.0.0",
  "description": "Adds gzip and brotli compression to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-compress"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "gzip",
    "brotli"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./brotli": {
      "import": "./lib/brotli.js",
      "default": "./lib/brotli.js"
    },
    "./gzip": {
      "import": "./lib/gzip.js",
      "default": "./lib/gzip.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "brotli": [
        "./lib/brotli.d.ts"
      ],
      "gzip": [
        "./lib/gzip.d.ts"
      ]
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.15.5"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "compression-webpack-plugin": "^10.0.0",
    "tslib": "2.5.0",
    "webpack": "5.76.2"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "compression-webpack-plugin": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "compression-webpack-plugin": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
