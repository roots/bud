        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.11.43"
  },
  "dependencies": {
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-entrypoints": "workspace:sources/@roots/bud-entrypoints",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-postcss": "workspace:sources/@roots/bud-postcss",
    "tslib": "2.4.0"
  },
  "peerDependencies": {
    "@roots/bud-babel": "*",
    "@roots/bud-entrypoints": "*",
    "@roots/bud-postcss": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud-babel": {
      "optional": true
    },
    "@roots/bud-entrypoints": {
      "optional": true
    },
    "@roots/bud-postcss": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
