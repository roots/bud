{
  "name": "@roots/bud",
  "version": "0.0.0",
  "description": "Frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "homepage": "https://roots.io/bud",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "clayrisser",
      "url": "https://github.com/clayrisser"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "roots",
    "webpack"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "docs/",
    "lib/",
    "vendor/"
  ],
  "bin": {
    "bud": "./bin/bud.mjs",
    "ts-bud": "./bin/ts-bud.mjs"
  },
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./bud": "./lib/bud.js",
    "./cli": "./lib/cli/index.js",
    "./cli/commands": "./lib/cli/commands/index.js",
    "./cli/commands/*": "./lib/cli/commands/*.js",
    "./context": "./lib/context/index.js",
    "./context/*": "./lib/context/*.js",
    "./extensions": "./lib/extensions/index.js",
    "./extensions/*": "./lib/extensions/*/index.js",
    "./factory": "./lib/factory/index.js",
    "./services/*": "./lib/services/*/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "bud": [
        "./lib/bud.d.ts"
      ],
      "cli": [
        "./lib/cli/index.d.ts"
      ],
      "cli/commands": [
        "./lib/cli/commands/index.d.ts"
      ],
      "cli/commands/*": [
        "./lib/cli/commands/*.d.ts"
      ],
      "context": [
        "./lib/context/index.d.ts"
      ],
      "context/*": [
        "./lib/context/*.d.ts"
      ],
      "extensions": [
        "./lib/extensions/index.d.ts"
      ],
      "extensions/*": [
        "./lib/extensions/*/index.d.ts"
      ],
      "factory": [
        "./lib/factory/index.d.ts"
      ],
      "services/*": [
        "./lib/services/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@repo/test-kit": "workspace:sources/@repo/test-kit",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.18.3",
    "@types/node-notifier": "8.0.2",
    "vitest": "0.24.5"
  },
  "dependencies": {
    "@roots/browserslist-config": "workspace:sources/@roots/browserslist-config",
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@roots/bud-build": "workspace:sources/@roots/bud-build",
    "@roots/bud-cache": "workspace:sources/@roots/bud-cache",
    "@roots/bud-compiler": "workspace:sources/@roots/bud-compiler",
    "@roots/bud-dashboard": "workspace:sources/@roots/bud-dashboard",
    "@roots/bud-extensions": "workspace:sources/@roots/bud-extensions",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-hooks": "workspace:sources/@roots/bud-hooks",
    "@roots/bud-server": "workspace:sources/@roots/bud-server",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/bud-terser": "workspace:sources/@roots/bud-terser",
    "@roots/container": "workspace:sources/@roots/container",
    "@roots/dependencies": "workspace:sources/@roots/dependencies",
    "browserslist": "4.21.4",
    "caniuse-lite": "1.0.30001429",
    "conf": "10.2.0",
    "dotenv": "16.0.3",
    "dotenv-expand": "9.0.0",
    "import-meta-resolve": "2.1.0",
    "node-notifier": "10.0.1",
    "open": "8.4.0",
    "open-editor": "4.0.0",
    "tslib": "2.4.1",
    "webpack": "5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "browserslist": "*",
    "caniuse-lite": "*"
  },
  "peerDependenciesMeta": {
    "@roots/browserslist-config": {
      "optional": true
    },
    "browserslist": {
      "optional": true
    },
    "caniuse-lite": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
