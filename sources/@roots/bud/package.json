{
  "name": "@roots/bud",
  "version": "0.0.0",
  "description": "Frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "engines": {
    "node": ">=16.0.0"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "Brandon",
      "email": "brandon@tendency.me",
      "url": "https://github.com/Log1x"
    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "roots",
    "webpack"
  ],
  "bin": {
    "bud": "./bin/bud.mjs",
    "bund": "./bin/bund.mjs",
    "ts-bud": "./bin/ts-bud.mjs"
  },
  "files": [
    "bin",
    "docs",
    "lib",
    "config"
  ],
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./bud": "./lib/bud.js",
    "./cli/commands/*": "./lib/cli/commands/*.js",
    "./cli/decorators/*": "./lib/cli/decorators/*.js",
    "./cli/flags": "./lib/cli/flags/index.js",
    "./cli/helpers/*": "./lib/cli/helpers/*.js",
    "./cli/*": "./lib/cli/*.js",
    "./context": "./lib/context/index.js",
    "./context/*": "./lib/context/*.js",
    "./factory": "./lib/factory.js",
    "./instance": "./lib/instance.js",
    "./services/*": "./lib/services/*.js",
    "./config/jsconfig.json": "./config/jsconfig.json",
    "./config/tsconfig.json": "./config/tsconfig.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "bud": [
        "./lib/bud.d.ts"
      ],
      "cli": [
        "./lib/cli/index.d.ts"
      ],
      "cli/commands": [
        "./lib/cli/commands/index.d.ts"
      ],
      "cli/decorators/*": [
        "./lib/cli/decorators/*.d.ts"
      ],
      "cli/flags": [
        "./lib/cli/flags/index.d.ts"
      ],
      "context": [
        "./lib/context/index.d.ts"
      ],
      "factory": [
        "./lib/factory.d.ts"
      ],
      "services/*": [
        "./lib/services/*.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.16.18"
  },
  "dependencies": {
    "@roots/browserslist-config": "workspace:*",
    "@roots/bud-api": "workspace:*",
    "@roots/bud-build": "workspace:*",
    "@roots/bud-cache": "workspace:*",
    "@roots/bud-client": "workspace:*",
    "@roots/bud-compiler": "workspace:*",
    "@roots/bud-dashboard": "workspace:*",
    "@roots/bud-entrypoints": "workspace:*",
    "@roots/bud-extensions": "workspace:*",
    "@roots/bud-framework": "workspace:*",
    "@roots/bud-hooks": "workspace:*",
    "@roots/bud-minify": "workspace:*",
    "@roots/bud-server": "workspace:*",
    "@roots/bud-support": "workspace:*",
    "tslib": "2.6.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
