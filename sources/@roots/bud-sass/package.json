{
  "name": "@roots/bud-sass",
  "version": "0.0.0",
  "description": "Sass support for @roots/bud projects.",
  "engines": {
    "node": ">=16"
  },
  "contributors": [
    {
      "email": "developers@tinypixel.dev",
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    },
    {
      "email": "ben@benword.com",
      "name": "Ben Word",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "email": "brandon@tendency.me",
      "name": "Brandon",
      "url": "https://github.com/Log1x"
    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-sass"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "roots",
    "sass"
  ],
  "files": [
    "lib",
    "config"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./extension": {
      "import": "./lib/extension.js",
      "default": "./lib/extension.js"
    },
    "./resolve-url": {
      "import": "./lib/resolve-url/index.js",
      "default": "./lib/resolve-url/index.js"
    },
    "./stylelint-config": {
      "require": "./config/stylelint.cjs",
      "default": "./config/stylelint.cjs"
    },
    "./config/stylelint": {
      "require": "./config/stylelint.cjs",
      "default": "./config/stylelint.cjs"
    },
    "./stylelint": {
      "require": "./config/stylelint.cjs",
      "default": "./config/stylelint.cjs"
    },
    "./types": {
      "import": "./lib/types.js",
      "default": "./lib/types.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ],
      "resolve-url": [
        "./lib/resolve-url/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@roots/bud-build": "workspace:*",
    "@roots/bud-postcss": "workspace:*",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.17.14",
    "postcss": "8.4.29",
    "stylelint": "15.10.3",
    "webpack": "5.88.2"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:*",
    "@roots/bud-support": "workspace:*",
    "@types/sass-loader": "8.0.5",
    "postcss-scss": "4.0.7",
    "resolve-url-loader": "5.0.0",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "stylelint-config-recommended-scss": "13.0.0",
    "stylelint-scss": "5.1.0",
    "tslib": "2.6.2"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
