{
  "name": "@roots/bud-sass",
  "version": "0.0.0",
  "description": "Sass support for @roots/bud projects.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-sass"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "sass"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "lib/",
    "stylelint-config/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./extension": "./lib/extension.js",
    "./resolve-url": "./lib/resolve-url/extension.js",
    "./stylelint-config": "./stylelint-config/index.cjs"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/types.d.ts",
        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ],
      "resolve-url": [
        "./lib/resolve-url/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@repo/test-kit": "workspace:sources/@repo/test-kit",
    "@roots/bud-build": "workspace:sources/@roots/bud-build",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.18.2",
    "postcss": "8.4.18",
    "stylelint": "14.14.0",
    "vitest": "0.24.4",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-postcss": "workspace:sources/@roots/bud-postcss",
    "postcss-scss": "^4.0.4",
    "resolve-url-loader": "5.0.0",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "tslib": "2.4.0"
  },
  "peerDependencies": {
    "@roots/bud-postcss": "*",
    "postcss-scss": "*",
    "sass": "*",
    "sass-loader": "*",
    "stylelint-config-recommended-scss": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud-postcss": {
      "optional": true
    },
    "postcss-scss": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "sass-loader": {
      "optional": true
    },
    "stylelint-config-recommended-scss": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
