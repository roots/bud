{
  "name": "@roots/bud-api",
  "version": "0.0.0",
  "description": "Bud configuration API",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-api"
  },
  "contributors": [
    {
      "name": "kellymears",
