{
  "name": "@roots/bud-api",
  "version": "5.8.1",
  "description": "Bud configuration API",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-api"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": "./lib/cjs/index.js",
    "./api": "./lib/cjs/api/index.js",
    "./methods": "./lib/cjs/methods/index.js",
    "./methods/*": "./lib/cjs/methods/*/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "api": [
        "./types/api/index.d.ts"
      ],
      "facade": [
        "./types/facade/index.d.ts"
      ],
      "methods": [
        "./types/methods/index.d.ts"
      ],
      "methods/*": [
        "./types/methods/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.11.26",
    "nanoid": "3.3.1",
    "webpack": "5.70.0"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "copy-webpack-plugin": "10.2.4",
    "css-minimizer-webpack-plugin": "3.4.1",
    "html-webpack-plugin": "5.5.0",
    "tslib": "2.3.1"
  }
}
