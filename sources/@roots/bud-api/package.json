{
  "name": "@roots/bud-api",
  "version": "5.1.0",
  "description": "Bud configuration API",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-api"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./repository": {
      "require": "./lib/cjs/repository/index.js",
      "import": "./lib/esm/repository/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "repository": [
        "./types/repository/index.d.ts"
      ],
      "repository/*": [
        "./types/repository/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@skypack/package-check": "0.2.2",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.22",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.5.0",
    "lodash": "4.17.21",
    "nanoid": "3.2.0",
    "npm-run-path": "5.0.1",
    "webpack": "5.67.0"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "copy-webpack-plugin": "10.2.4",
    "css-minimizer-webpack-plugin": "3.4.1",
    "tslib": "^2.3.1"
  }
}
