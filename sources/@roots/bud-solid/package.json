    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-solid"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "extension",
    "solid-js"
  ],
  "files": [
    "config",
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./types": {
      "import": "./lib/types.js",
      "default": "./lib/types.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@skypack/package-check": "0.2.2",
    "@types/babel__core": "7.20.1",
    "@types/node": "18.17.9"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.22.5",
    "@roots/bud": "workspace:*",
    "@roots/bud-babel": "workspace:*",
    "@roots/bud-framework": "workspace:*",
    "babel-preset-solid": "1.7.7",
    "solid-js": "1.7.11",
    "tslib": "2.6.2"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
