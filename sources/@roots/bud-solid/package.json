{
  "name": "@roots/bud-solid",
  "version": "0.0.0",
  "description": "solid-js support for @roots/bud projects.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-solid"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "solid-js"
  ],
  "engines": {
    "node": ">=14"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": "./lib/cjs/index.js",
    "./extension": "./lib/cjs/extension.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "extension": [
        "./types/extension.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@skypack/package-check": "0.2.2",
    "@types/lodash": "4.14.180",
    "@types/node": "16.11.26",
    "solid-js": "1.3.13",
    "type-fest": "2.12.1",
    "webpack": "5.70.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.16.7",
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "babel-preset-solid": "^1.2.5",
    "solid-js": "^1.1.7",
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "@babel/plugin-syntax-jsx": "*",
    "babel-preset-solid": "*",
    "solid-js": "*"
  },
  "peerDependenciesMeta": {
    "@babel/plugin-syntax-jsx": {
      "optional": true
    },
    "babel-preset-solid": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    }
  }
}
