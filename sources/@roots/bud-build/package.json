{
  "name": "@roots/bud-build",
  "description": "bud.js core module",
  "version": "0.0.0",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-build"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "docs/",
    "lib/",
    "src/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./item": {
      "types": "./lib/item/index.d.ts",
      "default": "./lib/item/index.js"
    },
    "./items": {
      "types": "./lib/handlers/items/items.d.ts",
      "default": "./lib/handlers/items/items.js"
    },
    "./loader": {
      "types": "./lib/loader/index.d.ts",
      "default": "./lib/loader/index.js"
    },
    "./loaders": {
      "types": "./lib/handlers/loaders/loaders.d.ts",
      "default": "./lib/handlers/loaders/loaders.js"
    },
    "./rule": {
      "types": "./lib/rule/index.d.ts",
      "default": "./lib/rule/index.js"
    },
    "./rules": {
      "types": "./lib/handlers/rules/index.d.ts",
      "default": "./lib/handlers/rules/index.js"
    }
  },
  "devDependencies": {
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@roots/bud-hooks": "workspace:sources/@roots/bud-hooks",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.18.11"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
