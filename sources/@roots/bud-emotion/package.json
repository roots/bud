{
  "name": "@roots/bud-emotion",
  "version": "0.0.0",
  "description": "CSS-in-JS support for the @roots/bud framework",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-emotion"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "emotion"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-babel",
      "@roots/bud-react"
    ]
  },
  "devDependencies": {
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-react": "workspace:sources/@roots/bud-react",
    "@skypack/package-check": "0.2.2",
    "@types/lodash": "4.14.179",
    "@types/node": "16.11.26",
    "react": "17.0.2"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "autobind-decorator": "^2.4.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0"
  },
  "peerDependenciesMeta": {
    "@emotion/babel-plugin": {
      "optional": true
    },
    "@emotion/css": {
      "optional": true
    },
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  }
}
