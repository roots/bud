{
  "name": "@roots/bud-emotion",
  "version": "5.0.0",
  "description": "CSS-in-JS support for the @roots/bud framework",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-emotion"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "emotion"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-babel",
      "@roots/bud-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@emotion/css": "11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-react": "workspace:sources/@roots/bud-react",
    "@skypack/package-check": "^0.2.2",
    "@types/eslint": "8.2.1",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.19",
    "jest": "27.4.7"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@emotion/css": "11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
