    "cors": "2.8.5",
    "express": "^4.17.1",
    "html-entities": "^2.1.0",
    "http-proxy-middleware": "2.0.4",
    "querystring": "^0.2.0",
    "strip-ansi": "^7.0.0",
    "tslib": "2.4.0",
    "webpack-dev-middleware": "5.3.1"
  }
}
