{
  "name": "@roots/bud-eslint",
  "version": "0.0.0",
  "description": "eslint support for bud.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-eslint"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "eslint"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./extension": "./lib/extension.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.js"
      ],
      "extension": [
        "./lib/extension.js"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.18",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "eslint": "8.31.0",
    "eslint-webpack-plugin": "^3.2.0"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "eslint": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "eslint": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
