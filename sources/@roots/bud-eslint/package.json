{
  "name": "@roots/bud-eslint",
  "version": "0.0.0",
  "description": "eslint support for bud.js",
  "engines": {
    "node": ">=16"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "Brandon",
      "email": "brandon@tendency.me",
      "url": "https://github.com/Log1x"
    },
    {
      "name": "Christian Widlund",
      "email": "christian.widlund@gmail.com",
      "url": "https://github.com/chrillep"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-eslint"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "eslint"
  ],
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./cache-fix": {
      "import": "./lib/cache-fix/index.js",
      "default": "./lib/cache-fix/index.js"
    },
    "./commands": {
      "import": "./lib/bud/commands.js",
      "default": "./lib/bud/commands.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "cache-fix": [
        "./lib/cache-fix/index.d.ts"
      ],
      "commands": [
        "./lib/bud/commands.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/eslint": "8.37.0",
    "@types/node": "18.16.12"
  },
  "dependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "eslint": "8.40.0",
    "eslint-webpack-plugin": "4.0.1",
    "tslib": "2.5.0",
    "webpack": "5.83.1"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
