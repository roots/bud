{
  "name": "@roots/bud-eslint",
  "version": "0.0.0",
  "description": "Adds eslint support to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-eslint"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "eslint"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "bud": {
    "type": "extension"
  },
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./extension": "./lib/extension.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.js"
      ],
      "extension": [
        "./lib/extension.js"
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "29.0.3",
    "@skypack/package-check": "0.2.2",
    "@types/lodash": "4.14.186",
    "@types/node": "16.11.68",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "eslint": "8.24.0",
    "eslint-webpack-plugin": "^3.1.1",
    "tslib": "2.4.0"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
