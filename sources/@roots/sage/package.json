{
  "name": "@roots/sage",
  "version": "0.0.0",
  "description": "@roots/sage preset for @roots/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/sage"
  },
  "documentation": "https://bud.js.org/extensions/sage",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "sage"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "config",
    "docs",
    "lib/",
    "stylelint-config"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./client/*": {
      "types": "./lib/client/*.d.ts",
      "default": "./lib/client/*.js"
    },
    "./config/stylelint": {
      "default": "./config/stylelint.cjs"
    },
    "./stylelint": {
      "default": "./config/stylelint.cjs"
    },
    "./stylelint-config": {
      "default": "./config/stylelint.cjs"
    },
    "./config/jsconfig": {
      "default": "./config/jsconfig.json"
    },
    "./config/tsconfig": {
      "default": "./config/tsconfig.cjs"
    },
    "./config/*": {
      "default": "./config/*"
    },
    "./*": {
      "types": "./lib/*/index.d.ts",
      "default": "./lib/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "client/*": [
        "./lib/*/dom-ready.d.ts"
      ],
      "*": [
        "./lib/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-tailwindcss": "workspace:sources/@roots/bud-tailwindcss",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.18.11",
    "tailwindcss": "3.2.4"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-preset-wordpress": "workspace:sources/@roots/bud-preset-wordpress",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container"
  },
  "peerDependencies": {
    "@roots/bud-preset-recommend": "*",
    "@roots/bud-preset-wordpress": "*",
    "@roots/bud-react": "*",
    "@roots/bud-tailwindcss": "*",
    "@roots/bud-wordpress-dependencies": "*",
    "@roots/bud-wordpress-externals": "*",
    "@roots/bud-wordpress-manifests": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud-preset-recommend": {
      "optional": true
    },
    "@roots/bud-preset-wordpress": {
      "optional": true
    },
    "@roots/bud-react": {
      "optional": true
    },
    "@roots/bud-tailwindcss": {
      "optional": true
    },
    "@roots/bud-wordpress-dependencies": {
      "optional": true
    },
    "@roots/bud-wordpress-externals": {
      "optional": true
    },
    "@roots/bud-wordpress-manifests": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
