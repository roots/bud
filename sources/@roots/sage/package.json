{
  "name": "@roots/sage",
  "version": "6.6.0",
  "description": "@roots/sage preset for @roots/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/sage"
  },
  "documentation": "https://bud.js.org/extensions/sage",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "sage"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "stylelint-config/",
    "tsconfig/",
    "lib/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./acorn": "./lib/acorn/index.js",
    "./acorn-v2-public-path": "./lib/acorn-v2-public-path/index.js",
    "./client": "./lib/client/index.js",
    "./client/dom-ready": "./lib/client/dom-ready.js",
    "./client/lazy": "./lib/client/lazy.js",
    "./wp-theme-json": "./lib/wp-theme-json/index.js",
    "./wp-theme-json-tailwind": "./lib/wp-theme-json-tailwind/index.js",
    "./stylelint-config": "./stylelint-config/index.cjs"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "acorn": [
        "./lib/acorn/index.d.ts"
      ],
      "acorn-v2-public-path": [
        "./lib/acorn-v2-public-path/index.d.ts"
      ],
      "client": [
        "./lib/client/index.d.ts"
      ],
      "client/dom-ready": [
        "./lib/client/dom-ready.d.ts"
      ],
      "client/lazy": [
        "./lib/client/lazy.d.ts"
      ],
      "wp-theme-json": [
        "./lib/wp-theme-json/index.d.ts"
      ],
      "wp-theme-json-tailwind": [
        "./lib/wp-theme-json-tailwind/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-tailwindcss": "workspace:sources/@roots/bud-tailwindcss",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.18.3",
    "tailwindcss": "3.2.4"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-preset-wordpress": "workspace:sources/@roots/bud-preset-wordpress",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "@roots/bud-preset-wordpress": "*",
    "@roots/bud-tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud-preset-wordpress": {
      "optional": true
    },
    "@roots/bud-tailwindcss": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}