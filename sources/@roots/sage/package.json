{
  "name": "@roots/sage",
  "version": "0.0.0",
  "description": "@roots/sage preset for @roots/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/sage"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "sage"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "stylelint-config/",
    "tsconfig/",
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js"
    },
    "./client": {
      "require": "./lib/cjs/client/index.js"
    },
    "./client/dom-ready": {
      "require": "./lib/cjs/client/dom-ready.js"
    },
    "./stylelint-config": {
      "require": "./stylelint-config/index.js"
    },
    "./theme/tailwind": {
      "require": "./lib/cjs/theme/tailwind.adapter.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "client": [
        "./types/client/index.d.ts"
      ],
      "client/dom-ready": [
        "./types/client/dom-ready.d.ts"
      ],
      "theme/tailwind": [
        "./types/theme/tailwind.adapter.d.ts"
      ]
    }
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-preset-wordpress"
    ]
  },
  "devDependencies": {
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.11.26",
    "webpack": "5.70.0"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-preset-wordpress": "workspace:sources/@roots/bud-preset-wordpress",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "tslib": "2.3.1"
  }
}
