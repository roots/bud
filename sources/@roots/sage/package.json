    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "sage"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "config",
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./acorn": {
      "types": "./lib/acorn/index.d.ts",
      "default": "./lib/acorn/index.js"
    },
    "./acorn-v2-public-path": {
      "types": "./lib/acorn-v2-public-path/index.d.ts",
      "default": "./lib/acorn-v2-public-path/index.js"
    },
    "./blade-loader": {
      "types": "./lib/blade-loader/index.d.ts",
      "default": "./lib/blade-loader/index.js"
    },
    "./client": {
      "types": "./lib/client/index.d.ts",
      "default": "./lib/client/index.js"
    },
    "./client/dom-ready": {
      "types": "./lib/client/dom-ready.d.ts",
      "default": "./lib/client/dom-ready.js"
    },
    "./client/lazy": {
      "types": "./lib/client/lazy.d.ts",
      "default": "./lib/client/lazy.js"
    },
    "./sage": {
      "types": "./lib/sage/index.d.ts",
      "default": "./lib/sage/index.js"
    },
    "./types": {
      "types": "./lib/types/index.d.ts",
      "default": "./lib/types/index.js"
    },
    "./stylelint": {
      "default": "./config/stylelint.cjs"
    },
    "./stylelint-config": {
      "default": "./config/stylelint.cjs"
    },
    "./config/stylelint": {
      "default": "./config/stylelint.cjs"
    },
    "./config/jsconfig.json": {
      "default": "./config/jsconfig.json"
    },
    "./config/tsconfig.json": {
      "default": "./config/tsconfig.json"
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "acorn": [
        "./lib/acorn/index.d.ts"
      ],
      "acorn-v2-public-path": [
        "./lib/acorn-v2-public-path/index.d.ts"
      ],
      "blade-loader": [
        "./lib/blade-loader/index.d.ts"
      ],
      "client": [
        "./lib/client/index.d.ts"
      ],
      "client/dom-ready": [
        "./lib/client/dom-ready.d.ts"
      ],
      "client/lazy": [
        "./lib/client/lazy.d.ts"
      ],
      "sage": [
        "./lib/sage/index.d.ts"
      ],
      "types": [
        "./lib/types/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.11.18"
  },
  "dependencies": {
    "@roots/blade-loader": "workspace:sources/@roots/blade-loader",
    "@roots/bud-build": "workspace:sources/@roots/bud-build",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-preset-wordpress": "workspace:sources/@roots/bud-preset-wordpress",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "@roots/bud-preset-wordpress": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "@roots/bud-preset-wordpress": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}