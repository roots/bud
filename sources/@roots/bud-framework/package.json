      "registry/*": [
        "./lib/types/registry/*.d.ts"
      ],
      "service": [
        "./lib/service.d.ts"
      ],
      "services": [
        "./lib/types/services/index.d.ts"
      ],
      "services/api": [
        "./lib/services/api.d.ts"
      ],
      "services/console": [
        "./lib/services/console.d.ts"
      ],
      "services/fs": [
        "./lib/services/fs.d.ts"
