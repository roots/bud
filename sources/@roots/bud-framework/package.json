{
  "name": "@roots/bud-framework",
  "version": "0.0.0",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-framework"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./bud": "./lib/bud.js",
    "./extension": "./lib/extension/index.js",
    "./extension/decorators": "./lib/extension/decorators/index.js",
    "./extension/decorators/*": "./lib/extension/decorators/*.js",
    "./methods": "./lib/methods/index.js",
    "./methods/*": "./lib/methods/*.js",
    "./module": "./lib/module.js",
    "./parsers": "./lib/parsers/index.js",
    "./parsers/*": "./lib/parsers/*.js",
    "./service": "./lib/service.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "bud": [
        "./lib/bud.d.ts"
      ],
      "config": [
        "./lib/config/index.d.ts"
      ],
      "extension": [
        "./lib/extension/index.d.ts"
      ],
      "extension/decorators": [
        "./lib/extension/decorators/index.d.ts"
      ],
      "extension/decorators/*": [
        "./lib/extension/decorators/*.d.ts"
      ],
      "methods": [
        "./lib/methods/index.d.ts"
      ],
      "methods/*": [
        "./lib/methods/*.d.ts"
      ],
      "parsers": [
        "./lib/parsers/index.d.ts"
      ],
      "parsers/*": [
        "./lib/parsers/*.d.ts"
      ],
      "service": [
        "./lib/service.d.ts"
      ],
      "services/*": [
        "./lib/services/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/fs-extra": "9.0.13",
    "@types/http-proxy": "1.17.9",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "4.17.6",
    "@types/node": "16.11.48",
    "@types/safe-json-stringify": "1.1.2",
    "@types/signale": "1.4.4",
    "webpack": "5.73.0"
  },
  "dependencies": {
    "@roots/container": "workspace:sources/@roots/container",
    "cli-highlight": "2.1.11",
    "figures": "4.0.1",
    "fs-extra": "10.1.0",
    "globby": "13.1.2",
    "helpful-decorators": "2.1.0",
    "import-meta-resolve": "2.0.3",
    "js-yaml": "4.1.0",
    "json5": "2.2.1",
    "lodash-es": "4.17.21",
    "pretty-format": "28.1.3",
    "safe-json-stringify": "1.2.0",
    "signale": "1.4.0",
    "tslib": "2.4.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
