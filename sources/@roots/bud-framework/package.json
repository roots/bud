{
  "name": "@roots/bud-framework",
  "version": "0.0.0",
  "description": "A friendly build tool to help manage your project assets.",
  "engines": {
    "node": ">=16"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "Brandon",
      "email": "brandon@tendency.me",
      "url": "https://github.com/Log1x"
    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-framework"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "files": [
    "docs",
    "lib",
    "scripts",
    "src",
    "vendor"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./bud": {
      "import": "./lib/bud.js",
      "default": "./lib/bud.js"
    },
    "./extension/decorators": {
      "import": "./lib/extension/decorators/index.js",
      "default": "./lib/extension/decorators/index.js"
    },
    "./extension/decorators/*": {
      "import": "./lib/extension/decorators/*.js",
      "default": "./lib/extension/decorators/*.js"
    },
    "./methods/*": {
      "import": "./lib/methods/*.js",
      "default": "./lib/methods/*.js"
    },
    "./methods": {
      "import": "./lib/methods/index.js",
      "default": "./lib/methods/index.js"
    },
    "./module": {
      "import": "./lib/module.js",
      "default": "./lib/module.js"
    },
    "./options": {
      "import": "./lib/types/options/index.js",
      "default": "./lib/types/options/index.js"
    },
    "./options/*": {
      "import": "./lib/types/options/*.js",
      "default": "./lib/types/options/*.js"
    },
    "./registry": {
      "import": "./lib/types/registry/index.js",
      "default": "./lib/types/registry/index.js"
    },
    "./registry/*": {
      "import": "./lib/types/registry/*.js",
      "default": "./lib/types/registry/*.js"
    },
    "./service": {
      "import": "./lib/service.js",
      "default": "./lib/service.js"
    },
    "./services": {
      "import": "./lib/types/services/index.js",
      "default": "./lib/types/services/index.js"
    },
    "./services/api": {
      "import": "./lib/services/api.js",
      "default": "./lib/services/api.js"
    },
    "./services/console": {
      "import": "./lib/services/console.js",
      "default": "./lib/services/console.js"
    },
    "./services/fs": {
      "import": "./lib/services/fs.js",
      "default": "./lib/services/fs.js"
    },
    "./services/notifier": {
      "import": "./lib/services/notifier/index.js",
      "default": "./lib/services/notifier/index.js"
    },
    "./services/build/*": {
      "import": "./lib/types/services/build/*.js",
      "default": "./lib/types/services/build/*.js"
    },
    "./services/*": {
      "import": "./lib/types/services/*.js",
      "default": "./lib/types/services/*/index.js"
    },
    "./value": {
      "import": "./lib/value.js",
      "default": "./lib/value.js"
    },
    "./*": {
      "import": "./lib/*/index.js",
      "default": "./lib/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "api": [
        "./lib/services/api.d.ts"
      ],
      "bud": [
        "./lib/bud/index.d.ts"
      ],
      "console": [
        "./lib/services/console.d.ts"
      ],
      "fs": [
        "./lib/services/fs.d.ts"
      ],
      "extension": [
        "./lib/extension/index.d.ts"
      ],
      "extension/decorators": [
        "./lib/extension/decorators/index.d.ts"
      ],
      "extension/decorators/*": [
        "./lib/extension/decorators/*.d.ts"
      ],
      "logger": [
        "./lib/logger/index.d.ts"
      ],
      "methods": [
        "./lib/methods/index.d.ts"
      ],
      "methods/*": [
        "./lib/methods/*.d.ts"
      ],
      "module": [
        "./lib/module.d.ts"
      ],
      "options": [
        "./lib/types/options/index.d.ts"
      ],
      "options/*": [
        "./lib/types/options/*.d.ts"
      ],
      "parsers": [
        "./lib/parsers/index.d.ts"
      ],
      "parsers/*": [
        "./lib/parsers/*.d.ts"
      ],
      "registry": [
        "./lib/types/registry/index.d.ts"
      ],
      "registry/*": [
        "./lib/types/registry/*.d.ts"
      ],
      "service": [
        "./lib/service.d.ts"
      ],
      "services": [
        "./lib/types/services/index.d.ts"
      ],
      "services/api": [
        "./lib/services/api.d.ts"
      ],
      "services/console": [
        "./lib/services/console.d.ts"
      ],
      "services/fs": [
        "./lib/services/fs.d.ts"
      ],
      "services/build/*": [
        "./lib/types/services/build/*.d.ts"
      ],
      "services/*": [
        "./lib/types/services/*/index.d.ts"
      ],
      "value": [
        "./lib/value.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.16.12"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container",
    "@roots/filesystem": "workspace:sources/@roots/filesystem",
    "chalk": "5.2.0",
    "execa": "7.1.1",
    "tslib": "2.5.0",
    "webpack": "5.83.0"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.mjs"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
