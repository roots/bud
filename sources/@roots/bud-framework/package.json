{
  "name": "@roots/bud-framework",
  "version": "6.4.1",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-framework"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./bud": "./lib/bud.js",
    "./extension": "./lib/extension/index.js",
    "./extension/decorators": "./lib/extension/decorators/index.js",
    "./extension/decorators/*": "./lib/extension/decorators/*.js",
    "./methods": "./lib/methods/index.js",
    "./methods/*": "./lib/methods/*.js",
    "./module": "./lib/module.js",
    "./parsers": "./lib/parsers/index.js",
    "./parsers/*": "./lib/parsers/*.js",
    "./service": "./lib/service.js",
    "./services/*": "./lib/services/*.js",
    "./value": "./lib/value.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "bud": [
        "./lib/bud.d.ts"
      ],
      "extension": [
        "./lib/extension/index.d.ts"
      ],
      "extension/decorators": [
        "./lib/extension/decorators/index.d.ts"
      ],
      "extension/decorators/*": [
        "./lib/extension/decorators/*.d.ts"
      ],
      "methods": [
        "./lib/methods/index.d.ts"
      ],
      "methods/*": [
        "./lib/methods/*.d.ts"
      ],
      "parsers": [
        "./lib/parsers/index.d.ts"
      ],
      "parsers/*": [
        "./lib/parsers/*.d.ts"
      ],
      "service": [
        "./lib/service.d.ts"
      ],
      "options": [
        "./lib/types/options/index.d.ts"
      ],
      "options/*": [
        "./lib/types/options/*.d.ts",
        "./lib/types/options/*/index.d.ts"
      ],
      "services": [
        "./lib/types/services/index.d.ts"
      ],
      "services/api": [
        "./lib/services/api.d.ts"
      ],
      "services/extensions": [
        "./lib/services/extensions.d.ts"
      ],
      "services/*": [
        "./lib/types/services/*.d.ts",
        "./lib/types/services/*/index.d.ts"
      ],
      "registry": [
        "./lib/types/registry/index.d.ts"
      ],
      "registry/*": [
        "./lib/types/registry/*.d.ts",
        "./lib/types/registry/*/index.d.ts"
      ],
      "value": [
        "./lib/value.d.ts"
      ],
      "config": [
        "./lib/types/config/index.d.ts"
      ],
      "config/*": [
        "./lib/types/config/*.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "29.0.1",
    "@skypack/package-check": "0.2.2",
    "@types/http-proxy": "1.17.9",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.11.48",
    "@types/safe-json-stringify": "1.1.2",
    "@types/signale": "1.4.4",
    "chokidar": "3.5.3",
    "http-proxy-middleware": "2.0.6",
    "webpack": "5.73.0"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container",
    "chalk": "5.0.1",
    "figures": "5.0.0",
    "globby": "13.1.2",
    "import-meta-resolve": "2.1.0",
    "js-yaml": "4.1.0",
    "json5": "2.2.1",
    "safe-json-stringify": "1.2.0",
    "signale": "1.4.0",
    "tslib": "2.4.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}