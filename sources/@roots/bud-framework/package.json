{
  "name": "@roots/bud-framework",
  "version": "0.0.0",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-framework"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js"
    },
    "./bud": {
      "require": "./lib/cjs/bud.js"
    },
    "./extension": {
      "require": "./lib/cjs/extension/extension.js"
    },
    "./module": {
      "require": "./lib/cjs/module.js"
    },
    "./service": {
      "require": "./lib/cjs/service.js"
    },
    "./services/*": {
      "require": "./lib/cjs/services/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "./config": [
        "./types/config/index.d.ts"
      ],
      "./extension": [
        "./types/extension/extension.d.ts"
      ],
      "./service": [
        "./types/service.d.ts"
      ],
      "./services/*": [
        "./types/services/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/lodash": "4.14.180",
    "@types/node": "16.11.26",
    "chokidar": "3.5.3",
    "http-proxy-middleware": "2.0.4",
    "webpack": "5.70.0",
    "webpack-dev-middleware": "5.3.1",
    "webpack-hot-middleware": "2.25.1"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container",
    "tslib": "2.3.1"
  }
}
