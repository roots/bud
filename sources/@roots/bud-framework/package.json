{
  "name": "@roots/bud-framework",
  "version": "0.0.0",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-framework"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./bud": {
      "types": "./lib/bud.d.ts",
      "default": "./lib/bud.js"
    },
    "./config": {
      "types": "./lib/types/config/index.d.ts",
      "default": "./lib/types/config/index.js"
    },
    "./configuration": {
      "types": "./lib/configuration/index.d.ts",
      "default": "./lib/configuration/index.js"
    },
    "./extension": {
      "types": "./lib/extension/index.d.ts",
      "default": "./lib/extension/index.js"
    },
    "./extension/decorators": {
      "types": "./lib/extension/decorators/index.d.ts",
      "default": "./lib/extension/decorators/index.js"
    },
    "./extension/decorators/*": {
      "types": "./lib/extension/decorators/*.d.ts",
      "default": "./lib/extension/decorators/*.js"
    },
    "./logger": "./lib/logger/index.js",
    "./methods": "./lib/methods/index.js",
    "./methods/*": "./lib/methods/*.js",
    "./module": "./lib/module.js",
    "./options": {
      "types": "./lib/types/options/index.d.ts",
      "default": "./lib/types/options/index.js"
    },
    "./options/*": {
      "types": "./lib/types/options/*.d.ts",
      "default": "./lib/types/options/*.js"
    },
    "./parsers": "./lib/parsers/index.js",
    "./parsers/*": "./lib/parsers/*.js",
    "./registry": {
      "types": "./lib/types/registry/index.d.ts",
      "default": "./lib/types/registry/index.js"
    },
    "./registry/*": {
      "types": "./lib/types/registry/*.d.ts",
      "default": "./lib/types/registry/*.js"
    },
    "./service": {
      "types": "./lib/service.d.ts",
      "default": "./lib/service.js"
    },
    "./services": {
      "types": "./lib/types/services/index.d.ts",
      "default": "./lib/types/services/index.js"
    },
    "./services/api": {
      "types": "./lib/services/api.d.ts",
      "default": "./lib/services/api.js"
    },
    "./services/console": {
      "types": "./lib/services/console.d.ts",
      "default": "./lib/services/console.js"
    },
    "./services/fs": {
      "types": "./lib/services/fs.d.ts",
      "default": "./lib/services/fs.js"
    },
    "./services/build/*": {
      "types": "./lib/types/services/build/*.d.ts",
      "default": "./lib/types/services/build/*.js"
    },
    "./services/*": {
      "types": "./lib/types/services/*/index.d.ts",
      "default": "./lib/types/services/*/index.js"
    },
    "./value": "./lib/value.js"
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/http-proxy": "1.17.9",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.18.4",
    "chokidar": "3.5.3",
    "http-proxy-middleware": "3.0.0-beta.0"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/container": "workspace:sources/@roots/container",
    "tslib": "2.4.1"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
