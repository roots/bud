{
  "name": "@roots/bud-stylelint",
  "version": "0.0.0",
  "description": "Adds stylelint support to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-stylelint"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "stylelint"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/",
    "config/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.cjs"
    },
    "./config": {
      "require": "./config/index.cjs",
      "default": "./config/index.js"
    },
    "./extension": {
      "import": "./lib/extension.js",
      "default": "./lib/extension.cjs"
    },
    "./bud/commands": {
      "import": "./lib/bud/commands/index.js",
      "default": "./lib/bud/commands/index.cjs"
    },
    "./types": {
      "import": "./lib/types.js",
      "default": "./lib/types.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.15.10",
    "webpack": "5.76.3"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "stylelint": "15.3.0",
    "stylelint-config-standard": "31.0.0",
    "stylelint-webpack-plugin": "4.1.0",
    "tslib": "2.5.0"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "stylelint": "*",
    "stylelint-config-standard": "*",
    "stylelint-scss": "*",
    "stylelint-webpack-plugin": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "stylelint-config-standard": {
      "optional": true
    },
    "stylelint-scss": {
      "optional": true
    },
    "stylelint-webpack-plugin": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
