{
  "name": "@roots/bud-criticalcss",
  "description": "Adds critical.css support to Bud",
  "version": "0.0.0",
  "bugs": "https://github.com/roots/bud/issues",
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@roots/critical-css-webpack-plugin": "workspace:sources/@roots/critical-css-webpack-plugin",
    "critical": "5.0.4",
    "tslib": "2.5.0",
    "vinyl": "3.0.0"
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.15.10",
    "@types/vinyl": "2.0.7"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./types": {
      "import": "./lib/types.js",
      "default": "./lib/types.js"
    }
  },
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "keywords": [
    "bud",
    "bud-extension",
    "criticalcss"
  ],
  "license": "MIT",
  "module": "./lib/index.js",
  "peerDependencies": {
    "@roots/bud": "*",
    "@roots/critical-css-webpack-plugin": "*",
    "critical": "*",
    "vinyl": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "@roots/critical-css-webpack-plugin": {
      "optional": true
    },
    "critical": {
      "optional": true
    },
    "vinyl": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-criticalcss"
  },
  "type": "module",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
