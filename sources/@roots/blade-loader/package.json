  "keywords": [
    "roots",
    "browserslist"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src",
    "lib",
    "vendor"
  ],
  "exports": {
    ".": {
      "types": "./lib/plugin.d.ts",
      "import": "./lib/plugin.js"
    },
    "./plugin": {
      "types": "./lib/plugin.d.ts",
