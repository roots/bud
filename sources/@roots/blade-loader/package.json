    "./script-loader": {
      "types": "./lib/script-loader.d.cts",
      "require": "./lib/script-loader.cjs"
    },
    "./asset-loader": {
      "types": "./lib/asset-loader.d.cts",
      "require": "./lib/asset-loader.cjs"
    }
  },
  "type": "module",
  "main": "./lib/plugin.js",
  "types": "./lib/plugin.d.ts",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "babel-loader": "9.1.2",
    "memfs": "3.4.13"
  },
  "dependencies": {
    "helpful-decorators": "2.1.0",
    "lodash": "4.17.21",
    "webpack": "5.75.0"
  },
  "volta": {
