{
  "name": "@roots/wordpress-theme-json-webpack-plugin",
  "description": "Webpack plugin to help with wp_enqueue_script",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/wordpress-theme-json-webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "webpack",
    "wordpress"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./theme": {
      "types": "./lib/theme.d.ts",
      "default": "./lib/theme.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "*": [
        "./lib/*.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.11.18"
  },
  "dependencies": {
    "fs-extra": "11.1.0",
    "helpful-decorators": "2.1.0",
    "lodash": "4.17.21",
    "webpack": "5.76.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
