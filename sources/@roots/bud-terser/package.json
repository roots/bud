{
  "name": "@roots/bud-terser",
  "version": "0.0.0",
  "description": "Adds terser support to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-terser"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "terser"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./extension": "./lib/extension.js",
    "./css-minimizer": "./lib/css-minimizer/extension.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "css-minimizer": [
        "./lib/css-minimizer/extension.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "29.0.3",
    "@repo/test-kit": "workspace:sources/@repo/test-kit",
    "@skypack/package-check": "0.2.2",
    "@types/lodash": "4.14.186",
    "@types/node": "16.18.0",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "css-minimizer-webpack-plugin": "4.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "tslib": "2.4.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
