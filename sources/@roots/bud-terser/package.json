{
  "name": "@roots/bud-terser",
  "version": "0.0.0",
  "description": "Adds terser support to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-terser"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "terser"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./css-minimizer": {
      "types": "./lib/css-minimizer/index.d.ts",
      "import": "./lib/css-minimizer/index.js"
    },
    "./types": {
      "types": "./lib/types.d.ts",
      "import": "./lib/types.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "css-minimizer": [
        "./lib/css-minimizer/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-swc": "workspace:sources/@roots/bud-swc",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.11.18"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "terser-webpack-plugin": "5.3.6"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "@roots/bud-support": "*",
    "terser-webpack-plugin": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "@roots/bud-support": {
      "optional": true
    },
    "terser-webpack-plugin": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
