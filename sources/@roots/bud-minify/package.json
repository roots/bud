{
  "name": "@roots/bud-minify",
  "version": "0.0.0",
  "description": "CSS and JS minification for bud.js",
  "engines": {
    "node": ">=16"
  },
  "contributors": [
    {
      "email": "developers@tinypixel.dev",
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-minify"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "extension"
  ],
  "files": [
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./extension": "./lib/extension/index.js",
    "./minify-css": "./lib/minify-css/index.js",
    "./minify-js": "./lib/minify-js/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension/index.d.ts"
      ],
      "minify-js": [
        "./lib/minify-js/index.d.ts"
      ],
      "minify-css": [
        "./lib/minify-css/index.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@roots/bud-swc": "workspace:*",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.18.3"
  },
  "dependencies": {
    "@roots/bud": "workspace:*",
    "@roots/bud-framework": "workspace:*",
    "@roots/bud-support": "workspace:*",
    "css-minimizer-webpack-plugin": "5.0.1",
    "lightningcss": "1.22.0",
    "terser": "5.21.0",
    "terser-webpack-plugin": "5.3.9",
    "tslib": "2.6.2",
    "webpack": "5.88.2"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
