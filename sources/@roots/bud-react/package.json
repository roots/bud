{
  "name": "@roots/bud-react",
  "version": "0.0.0",
  "description": "React support for @roots/bud projects.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-react"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "react"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js"
    },
    "./eslint-config": {
      "require": "./eslint-config/index.js"
    },
    "./extension": {
      "require": "./lib/cjs/extension.js"
    },
    "./react-refresh": {
      "require": "./lib/cjs/react-refresh/index.js"
    },
    "./react-refresh/*": {
      "require": "./lib/cjs/react-refresh/*.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "extension": [
        "./types/extension.d.ts"
      ],
      "react-refresh": [
        "./types/react-refresh/index.d.ts"
      ],
      "react-refresh/*": [
        "./types/react-refresh/*.d.ts"
      ]
    }
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-babel"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@skypack/package-check": "0.2.2",
    "@types/babel__core": "7.1.18",
    "@types/node": "16.11.26",
    "@types/react": "17.0.41",
    "@types/react-dom": "17.0.14",
    "type-fest": "2.12.1",
    "webpack": "5.70.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.11.0",
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
