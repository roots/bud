{
  "name": "@roots/bud-react",
  "version": "0.0.0",
  "description": "React support for @roots/bud projects.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "sources/@roots/bud-react"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "react"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./babel-refresh": "./lib/babel-refresh/index.js",
    "./extension": "./lib/extension.js",
    "./react-refresh": "./lib/react-refresh/index.js",
    "./react-refresh/*": "./lib/react-refresh/*.js",
    "./swc-refresh": "./lib/swc-refresh/index.js",
    "./typescript-refresh": "./lib/typescript-refresh/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "babel-refresh": [
        "./lib/babel-refresh/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ],
      "react-refresh": [
        "./lib/react-refresh/index.d.ts"
      ],
      "react-refresh/*": [
        "./lib/react-refresh/*.d.ts"
      ],
      "swc-refresh": [
        "./lib/swc-refresh/index.d.ts"
      ],
      "typescript-refresh": [
        "./lib/typescript-refresh/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@jest/globals": "29.0.3",
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@roots/bud-swc": "workspace:sources/@roots/bud-swc",
    "@roots/bud-typescript": "workspace:sources/@roots/bud-typescript",
    "@skypack/package-check": "0.2.2",
    "@types/babel__core": "7.1.19",
    "@types/node": "16.11.68",
    "@types/react": "18.0.22",
    "@types/react-dom": "18.0.7",
    "type-fest": "3.1.0",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.17.12",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.14.0",
    "tslib": "2.4.0"
  },
  "peerDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "*",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@pmmmwh/react-refresh-webpack-plugin": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
