{
  "name": "@roots/bud-mdx",
  "version": "5.1.0",
  "description": "mdx extension for @roots/bud projects",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-mdx"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "mdx"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-babel",
      "@roots/bud-react"
    ]
  },
  "devDependencies": {
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-react": "workspace:sources/@roots/bud-react",
    "@skypack/package-check": "0.2.2",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.22",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "webpack": "5.68.0"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@roots/bud-build": "workspace:sources/@roots/bud-build",
    "autobind-decorator": "^2.4.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@mdx-js/loader": "^1.6.22"
  },
  "peerDependenciesMeta": {
    "@mdx-js/loader": {
      "optional": true
    }
  }
}
