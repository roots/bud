    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "./lib/*": [
        "./lib/*.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "29.0.3",
    "@skypack/package-check": "0.2.2",
    "@types/node": "16.11.48",
    "@types/webpack-env": "1.18.0",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "strip-ansi": "7.0.1",
    "tslib": "2.4.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}