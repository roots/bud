{
  "name": "@roots/bud-client",
  "version": "0.0.0",
  "description": "Client scripts for @roots/bud",
  "engines": {
    "node": ">=16"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "Brandon",
      "email": "brandon@tendency.me",
      "url": "https://github.com/Log1x"
    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-client"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./lib/*": {
      "import": "./lib/*",
      "default": "./lib/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "./lib/*": [
        "./lib/*"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.mjs",
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.16.12",
    "@types/webpack-env": "1.18.0"
  },
  "dependencies": {
    "tslib": "2.5.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
