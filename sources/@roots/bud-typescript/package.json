{
  "name": "@roots/bud-typescript",
  "version": "0.0.0",
  "description": "Adds Typescript support to Bud.",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-typescript"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "keywords": [
    "bud",
    "bud-extension"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "config",
    "docs",
    "lib",
    "src",
    "tsconfig"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./typecheck": {
      "types": "./lib/typecheck/index.d.ts",
      "import": "./lib/typecheck/index.js"
    },
    "./bud/commands": {
      "types": "./lib/bud/commands/index.d.ts",
      "import": "./lib/bud/commands/index.js"
    },
    "./config/tsconfig.json": {
      "default": "./config/tsconfig.json"
    },
    "./tsconfig/tsconfig.json": {
      "default": "./config/tsconfig.json"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "typecheck": [
        "./lib/typecheck/index.d.ts"
      ]
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@skypack/package-check": "0.2.2",
    "@types/fork-ts-checker-webpack-plugin": "0.4.5",
    "@types/node": "18.11.18",
    "@types/webpack-env": "1.18.0",
    "react-refresh": "^0.14.0",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "react-refresh-typescript": "2.0.7",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "fork-ts-checker-webpack-plugin": "*",
    "react-refresh-typescript": "*",
    "ts-loader": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "fork-ts-checker-webpack-plugin": {
      "optional": true
    },
    "react-refresh-typescript": {
      "optional": true
    },
    "ts-loader": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}