      "import": "./lib/types.js",
      "default": "./lib/types.js"
    },
    "./config/tsconfig.json": "./config/tsconfig.json",
    "./tsconfig/tsconfig.json": "./config/tsconfig.json"
  },
  "files": [
    "config",
    "docs",
    "lib",
    "src",
    "tsconfig"
  ],
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension"
  ],
  "license": "MIT",
  "module": "./lib/index.js",
  "peerDependencies": {
    "@roots/bud": "*",
    "fork-ts-checker-webpack-plugin": "*",
    "react-refresh-typescript": "*",
    "ts-loader": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "@roots/bud": {
      "optional": true
    },
    "fork-ts-checker-webpack-plugin": {
      "optional": true
    },
    "react-refresh-typescript": {
      "optional": true
    },
    "ts-loader": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-typescript"
  },
  "type": "module",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "typecheck": [
        "./lib/typecheck/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
