{
  "name": "@roots/bud-typescript",
  "version": "0.0.0",
  "description": "Adds Typescript support to Bud.",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-typescript"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "keywords": [
    "bud",
    "bud-extension"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "docs/",
    "lib/",
    "tsconfig/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./bud/*": "./lib/bud/*.js",
    "./extension": "./lib/extension.js",
    "./tsconfig/tsconfig.json": "./tsconfig/tsconfig.json",
    "./typecheck": "./lib/typecheck/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ],
      "typecheck": [
        "./lib/typecheck/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@roots/bud": "workspace:sources/@roots/bud",
    "@skypack/package-check": "0.2.2",
    "@types/fork-ts-checker-webpack-plugin": "0.4.5",
    "@types/node": "16.18.6",
    "react-refresh": "^0.14.0",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "@types/webpack-env": "1.18.0",
    "chalk": "5.1.2",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "react-refresh-typescript": "2.0.7",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "fork-ts-checker-webpack-plugin": "*",
    "react-refresh-typescript": "*",
    "ts-loader": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "fork-ts-checker-webpack-plugin": {
      "optional": true
    },
    "react-refresh-typescript": {
      "optional": true
    },
    "ts-loader": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
