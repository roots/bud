{
  "name": "@roots/bud-typescript",
  "version": "0.0.0",
  "description": "Adds Typescript support to Bud.",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-typescript"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "keywords": [
    "bud",
    "bud-extension"
  ],
  "engines": {
    "node": ">=16"
  },
  "bud": {
    "type": "extension"
  },
  "files": [
    "lib/",
    "types/",
    "tsconfig/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./extension": "./lib/extension.js",
    "./tsconfig/tsconfig.json": "./tsconfig/tsconfig.json",
    "./typecheck": "./lib/typecheck/index.js",
    "./typecheck/extension": "./lib/typecheck/extension.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "extension": [
        "./lib/extension.d.ts"
      ],
      "typecheck": [
        "./lib/typecheck/index.d.ts"
      ],
      "typecheck/extension": [
        "./lib/typecheck/extension.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/fork-ts-checker-webpack-plugin": "0.4.5",
    "@types/node": "17.0.27",
    "webpack": "5.72.1"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@roots/bud-babel": "workspace:sources/@roots/bud-babel",
    "@types/webpack-env": "^1.16.3",
    "fork-ts-checker-webpack-plugin": "^7.2.6",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.8.0",
    "tslib": "2.4.0",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "fork-ts-checker-webpack-plugin": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "fork-ts-checker-webpack-plugin": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
