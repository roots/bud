    "@types/node": "16.11.26",
    "@types/signale": "1.4.4",
    "webpack": "5.70.0"
  },
  "dependencies": {
    "@roots/bud-support": "workspace:sources/@roots/bud-support",
    "postcss": "8.4.12",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.1",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "7.4.2",
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "postcss": "*",
    "postcss-import": "*",
    "postcss-nested": "*",
    "postcss-preset-env": "*"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    },
    "postcss-import": {
      "optional": true
