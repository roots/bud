  "exports": {
    ".": "./lib/index.js",
    "./css-loader": "./lib/css-loader/index.cjs",
    "./csv-loader": "./lib/csv-loader/index.cjs",
    "./file-loader": "./lib/file-loader/index.cjs",
    "./remark-loader": "./lib/remark-loader/index.cjs",
    "./style-loader": "./lib/style-loader/index.cjs",
    "./yml-loader": "./lib/yml-loader/index.cjs",
    "./decorators/*": "./lib/decorators/*.js",
    "./utilities/*": "./lib/utilities/*.js",
    "./highlight": "./vendor/highlight/index.js",
    "./html-loader": "./vendor/html-loader/index.cjs",
    "./html-webpack-plugin": "./vendor/html-webpack-plugin/index.cjs",
    "./*": "./lib/*/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "utilities/*": [
        "./lib/utilities/*.d.ts"
      ],
      "highlight": [
        "./vendor/highlight/index.d.ts"
      ],
      "html-webpack-plugin": [
        "./vendor/html-webpack-plugin/index.d.cts"
      ],
      "*": [
        "./lib/*/index.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
