    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*/index.d.ts",
      "default": "./lib/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "*": [
        "./lib/*/index.d.ts"
      ]
    }
  },
