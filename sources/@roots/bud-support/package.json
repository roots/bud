{
  "name": "@roots/bud-support",
  "version": "0.0.0",
  "description": "Internal packages for @roots/bud",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-support"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*/index.d.ts",
      "default": "./lib/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "*": [
        "./lib/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2"
  },
  "dependencies": {
    "@roots/filesystem": "workspace:sources/@roots/filesystem",
    "chalk": "5.1.2",
    "clean-stack": "5.1.0",
    "clipanion": "3.2.0-rc.14",
    "common-path": "1.0.1",
    "execa": "6.1.0",
    "figures": "5.0.0",
    "fs-extra": "10.1.0",
    "fs-jetpack": "5.1.0",
    "globby": "13.1.2",
    "helpful-decorators": "2.1.0",
    "highlight.js": "11.7.0",
    "import-meta-resolve": "2.2.0",
    "ink": "3.2.0",
    "ink-link": "2.0.0",
    "ink-text-input": "4.0.3",
    "lodash-es": "4.17.21",
    "parse5": "7.1.2",
    "parse5-htmlparser2-tree-adapter": "7.0.0",
    "patch-console": "1.0.0",
    "pretty-format": "29.3.1",
    "react": "17.0.2",
    "signale": "1.4.0",
    "typanion": "3.12.1",
    "type-fest": "3.3.0",
    "webpack": "5.75.0",
    "zod": "3.19.1"
  },
  "peerDependencies": {
    "webpack": ">=5.75.0"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    }
  }
}
