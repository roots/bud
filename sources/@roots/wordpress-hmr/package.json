      "types": "./lib/editor.d.ts",
      "default": "./lib/editor.js"
    },
    "./filter": {
      "types": "./lib/filter.d.ts",
      "default": "./lib/filter.js"
    },
    "./filters": {
      "types": "./lib/filters.d.ts",
      "default": "./lib/filters.js"
    },
    "./format": {
      "types": "./lib/format.d.ts",
      "default": "./lib/format.js"
    },
    "./formats": {
      "types": "./lib/formats.d.ts",
      "default": "./lib/formats.js"
    },
    "./loader": {
      "require": "./lib/loader.cjs"
    },
    "./plugin": {
      "types": "./lib/plugin.d.ts",
      "default": "./lib/plugin.js"
    },
    "./plugins": {
      "types": "./lib/plugins.d.ts",
      "default": "./lib/plugins.js"
    },
    "./variation": {
      "types": "./lib/variation.d.ts",
      "default": "./lib/variation.js"
    },
    "./variations": {
      "types": "./lib/variations.d.ts",
      "default": "./lib/variations.js"
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/wordpress__blocks": "12.0.2",
    "@types/wordpress__data": "6.0.1",
    "@types/wordpress__plugins": "3.0.0",
    "@types/wordpress__rich-text": "3.4.6",
    "@wordpress/blocks": "12.2.0",
    "@wordpress/data": "8.1.0",
    "@wordpress/hooks": "3.24.0",
    "@wordpress/plugins": "5.5.0",
    "@wordpress/rich-text": "6.4.0"
