{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "jsx": "react",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "strict": false,
    "target": "ES2021",
    "types": ["node", "jest", "react", "express", "webpack"]
  },
  "files": []
}
