{
  "name": "@roots/bud-eslint",
  "version": "3.0.14-alpha.0",
  "description": "Adds eslint support to Bud",
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/bud.git",
    "directory": "packages/extension-eslint"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "eslint"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "types/",
    "src/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc --build tsconfig.json",
    "build:esm": "tsc --build tsconfig-esm.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier 'src/**/*'  --write",
    "clean": "run-s clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:modules": "rimraf ./node_modules",
    "clean:types": "rimraf ./types",
    "pkg": "run-s pkg:*",
    "pkg:check": "package-check --verbose",
    "pkg:format": "prettier './lib/**/*.{js,ts}' --write"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "@roots/bud-support": "*",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-webpack-plugin": "^2.4.1"
  },
  "devDependencies": {
    "@roots/bud": "^3.0.14-alpha.0"
  },
  "dependencies": {
    "@roots/bud-support": "^3.0.14-alpha.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-webpack-plugin": "^2.4.1"
  },
  "gitHead": "9fb629f2bf9565389ba5daba35ff7759d384cd45"
}
