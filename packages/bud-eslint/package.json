{
  "name": "@roots/bud-eslint",
  "version": "2.0.0-next.24",
  "description": "Adds eslint support to Bud",
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/bud.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": ["bud-extension"],
  "engines": {
    "node": ">=12"
  },
  "files": ["lib"],
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "build": "run-s -s clean:* tsc",
    "clean:lib": "rimraf ./lib",
    "clean:modules": "rimraf ./node_modules",
    "clean:build": "rimraf ./tsconfig.build.tsbuildinfo",
    "tsc": "tsc -p tsconfig.build.json",
    "lint": "eslint ."
  },
  "dependencies": {
    "@roots/bud-support": "file:../bud-support",
    "babel-eslint": "8",
    "eslint": "^7.8.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8"
  },
  "gitHead": "a05ef3f17edacd8b52e4f885b91470e59becddf5",
  "devDependencies": {
    "@roots/bud": "file:../bud",
    "@roots/bud-types": "file:../bud-types",
    "@types/node": "^14.11.2"
  }
}
