{
  "name": "@roots/bud-imagemin",
  "version": "5.0.0-next.15",
  "description": "Image minification for `@roots/bud` projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "packages/@roots/bud-imagemin"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "imagemin"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "bud": {
    "type": "extension"
  },
  "devDependencies": {
    "@roots/bud-api": "workspace:packages/@roots/bud-api",
    "@roots/bud-framework": "workspace:packages/@roots/bud-framework",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.9",
    "jest": "27.3.1",
    "webpack": "5.64.2"
  },
  "dependencies": {
    "helpful-decorators": "^2.1.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin": "^8.0.1",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "10.0.0"
  },
  "peerDependenciesMeta": {
    "imagemin-gifsicle": {
      "optional": true
    },
    "imagemin-jpegtran": {
      "optional": true
    },
    "imagemin-optipng": {
      "optional": true
    },
    "imagemin-svgo": {
      "optional": true
    }
  },
  "stableVersion": "4.6.0",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ]
}
