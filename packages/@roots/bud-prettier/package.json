{
  "name": "@roots/bud-prettier",
  "version": "3.0.68-alpha.0",
  "description": "Adds prettier support to Bud",
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/bud.git",
    "directory": "packages/bud-prettier"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "prettier"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "run-s build:cjs build:esm",
    "build:cjs": "tsc --build tsconfig.json",
    "build:esm": "tsc --build tsconfig-esm.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier 'src/**/*'  --write",
    "clean": "run-s clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:modules": "rimraf ./node_modules",
    "pkg": "run-s pkg:*",
    "pkg:check": "package-check --verbose",
    "pkg:format": "prettier './lib/**/*.{js,ts}' --write"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1"
  },
  "gitHead": "2895cea534566264c5a908ad52b26a2eab1e5684",
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/webpack": "^4.41.26",
    "type-fest": "^0.21.2"
  }
}
