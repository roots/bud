{
  "name": "@roots/bud",
  "version": "4.7.0-next.12",
  "description": "A frontend build tooling framework combining the best parts of Symfony Encore and Laravel Mix",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/@roots/bud"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "clayrisser",
      "url": "https://github.com/clayrisser"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework",
    "webpack"
  ],
  "engines": {
    "node": ">=14"
  },
  "bin": "./bin/bud",
  "files": [
    "lib/",
    "bin/",
    "types/",
    "assets/",
    "vendor/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "oclif": {
    "commands": "./lib/cjs/cli/commands",
    "bin": "bud",
    "plugins": [
      "@oclif/plugin-update",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ],
    "topics": {
      "build": {
        "description": "compile project assets"
      },
      "build:dev": {
        "description": "compile assets for development"
      },
      "build:production": {
        "description": "compile assets for production"
      },
      "extensions": {
        "description": "get information on installed extensions"
      },
      "extensions:list": {
        "description": "list installed extensions"
      },
      "extensions:install": {
        "description": "install extension dependencies"
      },
      "clean": {
        "description": "clean project files"
      },
      "clean:storage": {
        "description": "clean project storage"
      },
      "clean:dist": {
        "description": "clean project distributables"
      },
      "clean:all": {
        "description": "clean all project files"
      }
    }
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "7.13.54",
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/css-minimizer-webpack-plugin": "3.0.2",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.175",
    "@types/mini-css-extract-plugin": "2.3.0",
    "@types/node": "15.14.1",
    "@types/pkg-up": "3.1.0",
    "@types/react": "17.0.27",
    "@types/resolve-pkg": "2.0.0",
    "@types/signale": "1.4.2",
    "@types/source-map-support": "0.5.4",
    "jest": "27.2.4",
    "tsconfig-paths": "3.11.0"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-plugins": "^1.10.1",
    "@oclif/plugin-update": "^1.3.10",
    "@roots/bud-api": "workspace:packages/@roots/bud-api",
    "@roots/bud-build": "workspace:packages/@roots/bud-build",
    "@roots/bud-cache": "workspace:packages/@roots/bud-cache",
    "@roots/bud-compiler": "workspace:packages/@roots/bud-compiler",
    "@roots/bud-dashboard": "workspace:packages/@roots/bud-dashboard",
    "@roots/bud-extensions": "workspace:packages/@roots/bud-extensions",
    "@roots/bud-framework": "workspace:packages/@roots/bud-framework",
    "@roots/bud-hooks": "workspace:packages/@roots/bud-hooks",
    "@roots/bud-server": "workspace:packages/@roots/bud-server",
    "@roots/bud-support": "workspace:packages/@roots/bud-support",
    "@roots/container": "workspace:packages/@roots/container",
    "@roots/dependencies": "workspace:packages/@roots/dependencies",
    "autobind-decorator": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "fs-extra": "^10.0.0",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.1.0",
    "signale": "^1.4.0",
    "source-map-support": "^0.5.19",
    "tslib": "^2.3.1",
    "webpack": "5.56.0",
    "webpack-config-dump-plugin": "^3.0.1",
    "webpack-manifest-plugin": "^4.0.2"
  },
  "stableVersion": "4.6.0"
}
