{
  "name": "@roots/bud-preset-recommend",
  "version": "4.6.0",
  "description": "Recommended preset for Bud projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "packages/@roots/bud-preset-recommend"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-preset"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "docs/",
    "lib/",
    "manifest.yml",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@roots/bud-api": "workspace:packages/@roots/bud-api",
    "@roots/bud-extensions": "workspace:packages/@roots/bud-extensions",
    "@roots/bud-framework": "workspace:packages/@roots/bud-framework",
    "@types/eslint": "^7.2.13",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.12.2"
  },
  "dependencies": {
    "@roots/bud": "workspace:packages/@roots/bud",
    "@roots/bud-babel": "workspace:packages/@roots/bud-babel",
    "@roots/bud-entrypoints": "workspace:packages/@roots/bud-entrypoints",
    "@roots/bud-eslint": "workspace:packages/@roots/bud-eslint",
    "@roots/bud-postcss": "workspace:packages/@roots/bud-postcss"
  },
  "peerDependencies": {
    "@roots/bud": "*",
    "@roots/bud-cli": "*",
    "eslint": "^7.27.0",
    "postcss": "^8.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
