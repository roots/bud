    "postcss": "8.3.11",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "6.7.0",
    "webpack": "5.62.1"
  },
  "dependencies": {
    "@roots/bud-build": "workspace:packages/@roots/bud-build",
    "postcss-loader": "6.2.0",
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "postcss": "8.3.11",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "6.7.0"
  },
  "peerDependenciesMeta": {
    "postcss-import": {
      "optional": true
    },
    "postcss-preset-env": {
      "optional": true
    }
