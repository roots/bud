    "@roots/bud-postcss": "workspace:*",
    "@roots/bud-preset-recommend": "workspace:*",
    "@roots/bud-prettier": "workspace:*",
    "@roots/bud-react": "workspace:*",
    "@roots/bud-stylelint": "workspace:*",
    "@roots/bud-support": "workspace:*",
    "@roots/bud-tailwindcss": "workspace:*",
    "@roots/bud-typescript": "workspace:*",
    "@roots/bud-wordpress-dependencies": "workspace:*",
    "@roots/bud-wordpress-externals": "workspace:*",
    "@roots/bud-wordpress-manifests": "workspace:*",
    "@roots/container": "workspace:*",
    "@roots/filesystem": "workspace:*",
    "@tailwindcss/jit": "^0.1.18",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.21",
    "tailwindcss": "^2.1.2"
