{
  "name": "@roots/sage",
  "version": "3.0.87-alpha.0",
  "description": "Sage starter theme build tooling",
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/bud.git",
    "directory": "packages/sage"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud-preset",
    "sage"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "bin/"
  ],
  "bin": "bin/sage.js",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./types/index.d.ts",
  "bud": {
    "commands": [
      "lib/esm/commands"
    ]
  },
  "scripts": {
    "build": "run-s build:cjs build:esm",
    "build:cjs": "tsc --build tsconfig.json",
    "build:esm": "tsc --build tsconfig-esm.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier 'src/**/*'  --write",
    "clean": "run-s clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:modules": "rimraf ./node_modules",
    "clean:types": "rimraf ./types",
    "pkg": "run-s pkg:*",
    "pkg:check": "package-check --verbose",
    "pkg:format": "prettier './lib/**/*.{js,ts}' --write"
  },
  "dependencies": {
    "@roots/bud": "^3.0.87-alpha.0",
    "@roots/bud-babel": "^3.0.87-alpha.0",
    "@roots/bud-cli": "^3.0.87-alpha.0",
    "@roots/bud-entrypoints": "^3.0.87-alpha.0",
    "@roots/bud-esbuild": "^3.0.87-alpha.0",
    "@roots/bud-eslint": "^3.0.87-alpha.0",
    "@roots/bud-postcss": "^3.0.87-alpha.0",
    "@roots/bud-preset-recommend": "^3.0.87-alpha.0",
    "@roots/bud-prettier": "^3.0.87-alpha.0",
    "@roots/bud-react": "^3.0.87-alpha.0",
    "@roots/bud-stylelint": "^3.0.87-alpha.0",
    "@roots/bud-tailwindcss": "^3.0.87-alpha.0",
    "@roots/bud-typescript": "^3.0.87-alpha.0",
    "@roots/bud-wordpress-dependencies": "^3.0.87-alpha.0",
    "@roots/bud-wordpress-externals": "^3.0.87-alpha.0",
    "@roots/bud-wordpress-manifests": "^3.0.87-alpha.0"
  },
  "gitHead": "2895cea534566264c5a908ad52b26a2eab1e5684",
  "devDependencies": {
    "@roots/bud-api": "^3.0.87-alpha.0",
    "@roots/bud-framework": "^3.0.87-alpha.0",
    "@types/node": "^14.14.31",
    "@types/webpack": "^4.41.26",
    "type-fest": "^0.21.2"
  }
}
