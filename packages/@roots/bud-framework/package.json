{
  "name": "@roots/bud-framework",
  "version": "4.6.0",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/@roots/bud-framework"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "docs/",
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "@types/webpack-dev-middleware": "^4.3.0",
    "chokidar": "^3.5.1",
    "http-proxy-middleware": "^2.0.0",
    "ink": "^3.0.8",
    "signale": "^1.4.0",
    "webpack": "^5.38.1",
    "webpack-dev-middleware": "^4.3.0"
  },
  "dependencies": {
    "@roots/container": "workspace:packages/@roots/container",
    "@roots/dependencies": "workspace:packages/@roots/dependencies",
    "@roots/filesystem": "workspace:packages/@roots/filesystem",
    "autobind-decorator": "^2.4.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "lodash": "^4.17.21"
  }
}
