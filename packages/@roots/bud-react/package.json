{
  "name": "@roots/bud-react",
  "version": "5.0.0",
  "description": "React support for @roots/bud projects.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "packages/@roots/bud-react"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "react"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "eslint-config/",
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": "./lib/cjs/index.js",
    "./eslint-config": "./eslint-config/index.js"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-babel"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@roots/bud-api": "workspace:packages/@roots/bud-api",
    "@roots/bud-babel": "workspace:packages/@roots/bud-babel",
    "@roots/bud-framework": "workspace:packages/@roots/bud-framework",
    "@types/babel__core": "7.1.18",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.17",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "jest": "27.4.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "type-fest": "2.8.0",
    "webpack": "5.65.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "autobind-decorator": "2.4.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react-hot-loader": "4.13.0",
    "react-refresh": "0.11.0",
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
