{
  "name": "@roots/bud-eslint",
  "version": "3.2.0-next.2",
  "description": "Adds eslint support to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "packages/@roots/bud-eslint"
  },
  "contributors": [
    "Kelly Mears <kelly@roots.io> (https://kellymears.me)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "eslint"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "docs/",
    "lib/",
    "types/",
    "manifest.yml"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.mjs"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@types/node": "^15.0.0",
    "webpack": "^5.36.0"
  },
  "dependencies": {
    "@roots/bud-extensions": "workspace:*",
    "@roots/bud-framework": "workspace:*",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4"
  }
}
