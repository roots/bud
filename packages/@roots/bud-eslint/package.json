{
  "name": "@roots/bud-eslint",
  "version": "5.0.0",
  "description": "Adds eslint support to Bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud",
    "directory": "packages/@roots/bud-eslint"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "eslint"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/",
    "template/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "bud": {
    "type": "extension"
  },
  "devDependencies": {
    "@roots/bud-framework": "workspace:packages/@roots/bud-framework",
    "@types/node": "16.11.14",
    "eslint": "7.32.0",
    "webpack": "5.65.0"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "eslint": "7.32.0",
    "prettier": "2.5.1"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  }
}
