{
  "name": "@roots/bud-emotion",
  "version": "5.0.0-next.17",
  "description": "CSS-in-JS support for the @roots/bud framework",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/@roots/bud-emotion"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-extension",
    "emotion"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "bud": {
    "type": "extension",
    "peers": [
      "@roots/bud-babel",
      "@roots/bud-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@emotion/css": "11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@roots/bud-babel": "workspace:packages/@roots/bud-babel",
    "@roots/bud-framework": "workspace:packages/@roots/bud-framework",
    "@roots/bud-react": "workspace:packages/@roots/bud-react",
    "@types/eslint": "7.29.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "jest": "27.4.4"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@emotion/css": "11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "stableVersion": "4.6.0"
}
